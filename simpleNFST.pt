# =============================================== 
# Define a simple NFST
# =============================================== 

# One start state
S = 'q0'

# List of final states
F = ['q1']

# List of tuple transitions (current_state, input, output, new_state)
T = [ ('q0','2','a','q0'),
      ('q0','2','b','q1'),
     ('q1','3','c','q1') ]

# Input as token list
Input = ['2','2','2','2','3']     


# =============================================== 
# Run 
# =============================================== 

tNFST(S,F,T,Input)

